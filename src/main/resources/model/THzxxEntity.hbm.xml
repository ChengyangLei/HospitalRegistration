<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.resources.model.THzxxEntity" table="t_hzxx" schema="hospital">
        <id name="hzbh">
            <column name="HZBH" sql-type="char(6)" length="6"/>
        </id>
        <property name="hzmc">
            <column name="HZMC" sql-type="char(12)" length="12"/>
        </property>
        <property name="pyzs">
            <column name="PYZS" sql-type="char(4)" length="4"/>
        </property>
        <property name="ksbh">
            <column name="KSBH" sql-type="char(6)" length="6"/>
        </property>
        <property name="sfzj">
            <column name="SFZJ" sql-type="tinyint(1)"/>
        </property>
        <property name="ghrs">
            <column name="GHRS" sql-type="int(11)"/>
        </property>
        <property name="ghfy">
            <column name="GHFY" sql-type="decimal(8,2)" precision="8" scale="2"/>
        </property>
    </class>
</hibernate-mapping>