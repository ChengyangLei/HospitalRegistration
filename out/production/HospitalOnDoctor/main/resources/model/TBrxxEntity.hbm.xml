<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.resources.model.TBrxxEntity" table="t_brxx" schema="hospital">
        <id name="brbh">
            <column name="BRBH" sql-type="char(6)" length="6"/>
        </id>
        <property name="brmc">
            <column name="BRMC" sql-type="char(10)" length="10"/>
        </property>
        <property name="dlkl">
            <column name="DLKL" sql-type="char(8)" length="8"/>
        </property>
        <property name="ycje">
            <column name="YCJE" sql-type="decimal(10,2)" precision="10" scale="2"/>
        </property>
        <property name="dlrq">
            <column name="DLRQ" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>