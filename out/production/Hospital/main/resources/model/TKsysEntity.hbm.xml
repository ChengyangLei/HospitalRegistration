<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.resources.model.TKsysEntity" table="t_ksys" schema="hospital">
        <id name="ysbh">
            <column name="YSBH" sql-type="char(6)" length="6"/>
        </id>
        <property name="ksbh">
            <column name="KSBH" sql-type="char(6)" length="6"/>
        </property>
        <property name="ysmc">
            <column name="YSMC" sql-type="char(10)" length="10"/>
        </property>
        <property name="ptzs">
            <column name="PTZS" sql-type="char(4)" length="4"/>
        </property>
        <property name="dlkl">
            <column name="DLKL" sql-type="char(8)" length="8"/>
        </property>
        <property name="sfzj">
            <column name="SFZJ" sql-type="tinyint(1)"/>
        </property>
        <property name="dlrq">
            <column name="DLRQ" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>