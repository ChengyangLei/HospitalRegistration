<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.resources.model.TKsxxEntity" table="t_ksxx" schema="hospital">
        <id name="ksbh">
            <column name="KSBH" sql-type="char(6)" length="6"/>
        </id>
        <property name="ksmc">
            <column name="KSMC" sql-type="char(10)" length="10"/>
        </property>
        <property name="pyzs">
            <column name="PYZS" sql-type="char(8)" length="8"/>
        </property>
    </class>
</hibernate-mapping>